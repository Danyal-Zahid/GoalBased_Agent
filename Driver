from Node import Node
MNT = input().split(' ')
M = int(MNT[0])
N = int(MNT[1])
T = int(MNT[2])

states = []
actions = []

input()
for i in range(M):
    states.append(input())

input()
for i in range(N):
    actions.append(input())

input()

transitionTable = []
for i in range(M):
    temp = input().split(' ')
    transitionTable.append([])
    for j in range(N):
        transitionTable[i].append(int(temp[j]))

input()

testCases = []
for i in range(T):
    testCases.append(input().split('\t'))
